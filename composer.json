{
    "name": "naif/mailchimp-tool",
    "description": "A Laravel Nova tool that integrate with MailChimp to add/edit subscribe and send email messages.",
    "keywords": [
        "laravel",
        "nova",
        "MailChimp",
        "tool"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\MailchimpTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\MailchimpTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
